        "editorMarkerNavigationError.background": "#ff2c6d",
        "editorMarkerNavigationInfo.background": "#45a9f9",
        "editorMarkerNavigationWarning.background": "#ffb86c",
        // Suggest Widget
        "editorSuggestWidget.background": "#f8f8f8",
        "editorSuggestWidget.border": "#b0b0b0",
        "editorSuggestWidget.focusHighlightForeground": "#45a9f9",
        "editorSuggestWidget.foreground": "#383848",
        "editorSuggestWidget.highlightForeground": "#45a9f9",
        "editorSuggestWidget.selectedBackground": "#dce4f0",
        "editorSuggestWidget.selectedForeground": "#383848",
        // Hover Widget
        "editorHoverWidget.background": "#f8f8f8",
        "editorHoverWidget.border": "#b0b0b0",
        "editorHoverWidget.foreground": "#383848",
        // Peek View
        "peekView.border": "#d8d8d8",
        "peekViewEditor.background": "#ffffff",
        "peekViewEditor.matchHighlightBackground": "#fae080",
        "peekViewEditor.matchHighlightBorder": "#ddc87b",
        "peekViewEditorGutter.background": "#ffffff",
        "peekViewResult.background": "#f8f8f8",
        "peekViewResult.fileForeground": "#383848",
        "peekViewResult.lineForeground": "#383848",
        "peekViewResult.matchHighlightBackground": "#fae080",
        "peekViewResult.selectionBackground": "#dce4f0",
        "peekViewResult.selectionForeground": "#383848",
        "peekViewTitle.background": "#e8e8e8",
        "peekViewTitleDescription.foreground": "#767680",
        "peekViewTitleLabel.foreground": "#383848",
        // Editor
        "editor.background": "#ffffff",
        "editor.findMatchBackground": "#b8d9ff",
        "editor.findMatchBorder": "#4c95e9",
        "editor.findMatchHighlightBackground": "#f1e7bc",
        "editor.findRangeHighlightBackground": "#4c95e922",
        "editor.foldBackground": "#f8f8f8",
        "editor.foreground": "#3e3e3e",
        "editor.hoverHighlightBackground": "#f4f8f8",
        "editor.inactiveSelectionBackground": "#b8d9ff",
        "editor.lineHighlightBackground": "#f4f8f8",
        "editor.rangeHighlightBackground": "#f1e7bcaa",
        "editor.selectionBackground": "#b8d9ff",
        "editor.selectionForeground": "#3e3e3e00", // disabled
        "editor.selectionHighlightBackground": "#dae4f7",
        "editorBracketMatch.background": "#e4d5f8",
        "editorBracketMatch.border": "#e4d5f800", // disabled
        "editorCodeLens.foreground": "#949494",
        "editorCursor.background": "#ffffff",
        "editorCursor.foreground": "#000000",
        "editorGutter.background": "#ffffff",
        "editorIndentGuide.activeBackground": "#a0a0a0",
        "editorIndentGuide.background": "#d8d8d8",
        "editorLineNumber.activeForeground": "#898989",
        "editorLineNumber.foreground": "#898989",
        "editorWhitespace.foreground": "#f1f1f1",
