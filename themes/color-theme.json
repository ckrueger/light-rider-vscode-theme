        "tab.unfocusedHoverForeground": "#383848",
        "tab.unfocusedInactiveBackground": "#e8e8e8",
        "tab.unfocusedInactiveForeground": "#383848",
        "tab.unfocusedInactiveModifiedBorder": "#e8e8e800", //disabled
